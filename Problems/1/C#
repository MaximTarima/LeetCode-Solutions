public class Solution {
    public int[] TwoSum(int[] nums, int target) {
        Dictionary<int, int> viewed = new Dictionary<int, int>();
        for (int i = 0; i < nums.Length; i++) {
            int pair = target - nums[i];
            if (viewed.TryGetValue(pair, out int j))
                return new int[]{i, j};
            if (!viewed.ContainsKey(pair))
        }
    }
}